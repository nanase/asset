{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "array",
  "title": "KemoV YouTube Channel",
  "items": {
    "type": "object",
    "required": [
      "id",
      "fullname",
      "color"
    ],
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "fullname": {
        "type": "string"
      },
      "globalname": {
        "type": "string"
      },
      "twitter": {
        "type": "string"
      },
      "twitch": {
        "type": "string"
      },
      "color": {
        "type": "object",
        "required": [
          "key",
          "sub",
          "light",
          "back"
        ],
        "properties": {
          "key": {
            "type": "string"
          },
          "sub": {
            "type": "string"
          },
          "light": {
            "type": "string"
          },
          "back": {
            "type": "string"
          }
        }
      }
    }
  }
}
